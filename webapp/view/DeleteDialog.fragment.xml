<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:f="sap.ui.layout.form" xmlns:sap.ui.layout="sap.ui.layout">
	<Dialog id="dialogDelComm" title="Inserisci dettagli" stretch="true">
		<html:style>
			.sapMILILabel { font-size: 14px; !important }
		</html:style>
		<content>
			<VBox class="sapUiSmallMargin">
				<f:Form id="FormToolbarDelComm" editable="true" ariaLabelledBy="Title1">
					<f:layout>
						<f:ResponsiveGridLayout labelSpanXL="4" labelSpanL="3" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="4"
							emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="1" columnsM="1" singleContainerFullSize="false"/>
					</f:layout>
					<f:formContainers>
						<f:FormContainer ariaLabelledBy="Title2">
							<f:toolbar>
								<Toolbar>
									<Title id="dettaglioComm" text="Dettaglio commessa selezionata" level="H5" titleStyle="H5"/>
									<ToolbarSpacer/>
								</Toolbar>
							</f:toolbar>
							<f:formElements>
								
								<f:FormElement label="Commessa">
									<f:fields>
										<Text id="commessaSelDel" text="{Orderjob}"/>
									</f:fields>
								</f:FormElement>
								<f:FormElement label="Sede">
									<f:fields>
										<Text id="sedeSel" text="{Office}"></Text>
									</f:fields>
								</f:FormElement>
								<f:FormElement label="Ore">
									<f:fields>
										<Text id="oreSel" text="{Ore}"/>
									</f:fields>
								</f:FormElement>
								<f:FormElement label="Descrizione">
									<f:fields>
										<TextArea id="descrizioneSel" width="100%" height="3rem" wrapping="None" maxLength="200"
											tooltip="Descrizione attivitÃ ." value="{Descr}" enabled="false"/>
									</f:fields>
								</f:FormElement>
								<f:FormElement label="Chilometri effettuati">
									<f:fields>
										<Text id="chilometriSel" text="{Km}"/>
									</f:fields>
								</f:FormElement>
								<f:FormElement label="Descrizione chilometri">
									<f:fields>
										<TextArea id="descrizioneChilometriSel" width="100%" height="3rem" wrapping="None" maxLength="200"
											tooltip="Descrizione chilometri." value = "{Expdescr}" enabled="false"/>
									</f:fields>
								</f:FormElement>
							</f:formElements>
							<f:formElements>
								<f:FormElement>
									<f:fields>
										<Panel id="panelSpeseSel" expandable="true" expanded="false" headerText="Spese" width="auto" class="sapUiResponsiveMargin">
											<content>
												<Table id="tabellaSpeseSel" fixedLayout="true" growingThreshold="10" mode="MultiSelect" selectionChange="onExpenseSelect">
													<columns>
														<Column>
															<Text text="Spesa"/>
														</Column>
														<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
															<Text text="Descrizione"/>
														</Column>
														<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
															<Text text="Importo (EUR)"/>
														</Column>
													</columns>
													<items>
													
														<ColumnListItem>
															<cells>
																<ObjectIdentifier title="01 - Autostrada (EUR)"/>
																<TextArea enabled ="false" tooltip="descrizione spesa" textAlign="Begin" width="auto" maxLength="200" placeholder="descrizione spesa (opzionale)"/>
																<Input tooltip="Autostrada" type="Number" width="4rem" enabled="false" liveChange="onLiveChange"/>
															</cells>
														</ColumnListItem>
														<ColumnListItem>
															<cells>
																<ObjectIdentifier title="02 - Cena (EUR)"/>
																<TextArea enabled ="false" tooltip="descrizione spesa" textAlign="Begin" width="auto" maxLength="200" placeholder="descrizione spesa (opzionale)"/>
																<Input tooltip="Cena" type="Number" width="4rem" enabled="false" liveChange="onLiveChange"/>
															</cells>
														</ColumnListItem>
														<ColumnListItem>
															<cells>
																<ObjectIdentifier title="03 - Treno (EUR)"/>
																<TextArea enabled ="false" tooltip="descrizione spesa" textAlign="Begin" width="auto" maxLength="200" placeholder="descrizione spesa (opzionale)"/>
																<Input tooltip="Treno" type="Number" width="4rem" enabled="false" liveChange="onLiveChange"/>
															</cells>
														</ColumnListItem>
														<ColumnListItem>
															<cells>
																<ObjectIdentifier title="04 - Albergo (EUR)"/>
																<TextArea enabled ="false" tooltip="descrizione spesa" textAlign="Begin" width="auto" maxLength="200" placeholder="descrizione spesa (opzionale)"/>
																<Input tooltip="Albergo" type="Number" width="4rem" enabled="false" liveChange="onLiveChange"/>
															</cells>
														</ColumnListItem>
														<ColumnListItem>
															<cells>
																<ObjectIdentifier title="05 - Taxi/BUS (EUR)"/>
																<TextArea enabled ="false" tooltip="descrizione spesa" textAlign="Begin" width="auto" maxLength="200" placeholder="descrizione spesa (opzionale)"/>
																<Input tooltip="Taxi/BUS" type="Number" width="4rem" enabled="false" liveChange="onLiveChange"/>
															</cells>
														</ColumnListItem>
														<ColumnListItem>
															<cells>
																<ObjectIdentifier title="06 - Aereo (EUR)"/>
																<TextArea enabled ="false" tooltip="descrizione spesa" textAlign="Begin" width="auto" maxLength="200" placeholder="descrizione spesa (opzionale)"/>
																<Input tooltip="Aereo" type="Number" width="4rem" enabled="false" liveChange="onLiveChange"/>
															</cells>
														</ColumnListItem>
														<ColumnListItem>
															<cells>
																<ObjectIdentifier title="07 - Parcheggio (EUR)"/>
																<TextArea enabled ="false" tooltip="descrizione spesa" textAlign="Begin" width="auto" maxLength="200" placeholder="descrizione spesa (opzionale)"/>
																<Input tooltip="Parcheggio" type="Number" width="4rem" enabled="false" liveChange="onLiveChange"/>
															</cells>
														</ColumnListItem>
														<ColumnListItem>
															<cells>
																<ObjectIdentifier title="08 - Pranzo (EUR)"/>
																<TextArea enabled ="false" tooltip="descrizione spesa" textAlign="Begin" width="auto" maxLength="200" placeholder="descrizione spesa (opzionale)"/>
																<Input tooltip="Pranzo" type="Number" width="4rem" enabled="false" liveChange="onLiveChange"/>
															</cells>
														</ColumnListItem>
														<ColumnListItem>
															<cells>
																<ObjectIdentifier title="09 - Altro (EUR)"/>
																<TextArea enabled ="false" tooltip="descrizione spesa" textAlign="Begin" width="auto" maxLength="200" placeholder="descrizione spesa (opzionale)"/>
																<Input tooltip="Altro" type="Number" width="4rem" enabled="false" liveChange="onLiveChange"/>
															</cells>
														</ColumnListItem>
													</items>
												</Table>
											</content>
										</Panel>
									</f:fields>
								</f:FormElement>
							</f:formElements>
						</f:FormContainer>
					</f:formContainers>
				</f:Form>
			</VBox>
		</content>
		<beginButton>
			<Button id="confermaSel" text="Conferma" press="onConfirmation" type="Accept"/>
			<!--Fare Check su ore !-->
		</beginButton>
		<endButton>
			<Button text="Indietro" press="closeDialogSel" type="Reject"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>